<% content_for :mainclass do %>
    cover
<% end %>


<!-- <h2 class="form-signin-heading">log in</h2> -->
<div class='cover-element'>
<%= form_for(resource, as: resource_name, url: session_path(resource_name), html:{class:'form-signin'}) do |f| %>
  <div><!-- <%= f.label :email %><br /> -->
  <%= f.email_field :email, autofocus: true , placeholder:'Email address', class:"form-control top"%></div>

  <div><!-- <%= f.label :password %><br /> -->
    <%= f.password_field :password, autocomplete: "off", placeholder:'Password', class:"form-control bottom"%></div>

  <% if devise_mapping.rememberable? -%>
    <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
  <% end -%>

  <div><%= f.submit "Login" , class:"btn btn-lg btn-block custbtn"%></div>
<% end %>

<%= render "devise/shared/links" %>
</div>